extends layout
block content
    .row
        .col-md-12
            table.table.table-bordered.text-left
                thead
                    h3 #{title} Details
                each val, key in college
                    tr
                        td.lead=key
                        td=val
        .col-md-12
            table.table.table-bordered.text-left
                thead
                    h3.alert-danger.text-center.well-lg College Gender Distribution
                    tr
                        p.lead Gender Percentage

                    tr
                        #gender
                        script(src='/js/lib/c3.min.js')
                        script(src='/js/lib/d3.min.js')
                        script(src='/js/lib/visuals.js')
                    tr

                        p.lead Gender Information

                    tr
                        #chart

                    tr
                        p.lead Enrollment Data
                        #tuitionchart
